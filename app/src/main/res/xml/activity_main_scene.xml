<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnSwipe
            motion:touchAnchorId="@+id/imageView"
            motion:touchAnchorSide="bottom" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/imageView"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="408dp"
            android:layout_height="322dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintHorizontal_bias="0.333"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent">
            <CustomAttribute
                motion:attributeName="roundPercent"
                motion:customFloatValue="0" />
        </Constraint>
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView"
            android:layout_marginBottom="39dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView2"
            motion:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginTop="8dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView3"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            android:layout_marginTop="32dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView5"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/textView3"
            motion:layout_editor_absoluteY="373dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView6"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"
            motion:layout_constraintTop_toBottomOf="@+id/textView5"
            motion:layout_editor_absoluteY="392dp"
            android:layout_marginTop="8dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/switch1"
            motion:layout_constraintTop_toBottomOf="@+id/divider"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginTop="16dp"
            motion:layout_constraintBottom_toBottomOf="@+id/textView4" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView7"
            motion:layout_constraintTop_toBottomOf="@+id/textView4"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteX="17dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView4"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteX="29dp"
            motion:layout_constraintTop_toBottomOf="@+id/divider"
            android:layout_marginTop="8dp"
            android:layout_marginStart="16dp" />
        <Constraint
            android:layout_height="1dp"
            android:layout_width="409dp"
            android:id="@+id/divider"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/textView6"
            motion:layout_editor_absoluteY="427dp"
            android:layout_marginTop="16dp"
            android:layout_marginStart="0dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/qr"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="@+id/textView5"
            motion:layout_constraintBottom_toBottomOf="@+id/textView5"
            android:layout_marginEnd="24dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/arrow"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/more"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp"
            android:layout_marginTop="16dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/imageView"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="65dp"
            android:layout_height="65dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.063"
            android:layout_marginStart="16dp"
            android:layout_marginTop="32dp"
            android:layout_marginBottom="32dp">
            <CustomAttribute
                motion:attributeName="roundPercent"
                motion:customFloatValue="1" />
        </Constraint>
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView"
            motion:layout_constraintStart_toEndOf="@+id/imageView"
            motion:layout_constraintTop_toTopOf="@+id/imageView"
            android:layout_marginStart="16dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@+id/textView"
            android:layout_marginTop="0dp"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView"
            android:id="@+id/textView2" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/switch1"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp"
            android:layout_marginRight="36dp"
            motion:layout_constraintTop_toBottomOf="@+id/divider"
            android:layout_marginTop="16dp" />
        <Constraint
            android:layout_height="30dp"
            android:layout_width="26dp"
            android:id="@+id/qr"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="@+id/textView5"
            motion:layout_constraintBottom_toBottomOf="@+id/textView5"
            android:layout_marginEnd="32dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/arrow"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/more"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="16dp"
            android:layout_marginTop="16dp" />
    </ConstraintSet>
</MotionScene>